    "clsx": "^2.1.0",
    "framer-motion": "^11.0.6",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "lucide-react": "^0.338.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-awesome-reveal": "^4.2.8",
    "react-dom": "^18",
    "react-hydration-provider": "^2.1.0",
